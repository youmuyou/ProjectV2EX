/*markdown样式*/

.md_t1 {
  font-size: 44rpx;
  margin: 24rpx 0;
}
.md_t2 {
  font-size: 40rpx;
  margin: 24rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
  padding-bottom: 12rpx;
}

.md_t3 {
  font-size: 36rpx;
  margin: 12rpx 0;
}

.md_t4 {
  font-size: 34rpx;
  margin: 24rpx 0;
}

.md_listitem {
  margin-left: 4rpx;
}

.md_text {
  font-size: 30rpx;
  margin: 16rpx 0;
}

.md_pre {
  font-size: 25rpx;
  background: #f5f5f5;
  margin: 20rpx 0;
  padding: 6rpx;
  max-width: 100%;
  word-wrap: break-word;
  white-space: pre;
  overflow-x: auto;
  overflow-y: auto;
}

.md_blockquote {
  text-indent: 2em;
  margin: 8rpx 0;
  background: #fff8dd;
  padding: 8rpx;
  font-size: 25rpx;
  border-radius: 4rpx;
}

.md_blockquote .md_text {
  font-size: 25rpx;
}

.md_link {
  word-break: break-word;
}
.embedded_image{
  max-width: 100%;
}